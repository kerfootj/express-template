{
    "name": "express-template",
    "version": "1.0.0",
    "description": "Basic Typescript Express started template with Docker. This template is meant to be used as a minimal starting point technical interviews.",
    "type": "module",
    "main": "dist/server.js",
    "scripts": {
        "dev": "docker compose up",
        "start:dev": "nodemon -I --inspect=0.0.0.0:9229 --exec node --es-module-specifier-resolution=node --no-warnings=ExperimentalWarning --loader ts-node/esm src/server.ts",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "keywords": [
        "express",
        "typescript",
        "template",
        "starter"
    ],
    "author": "Joel Kerfoot",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@jest-mock/express": "^2.0.2",
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.20",
        "@types/jest": "^29.5.7",
        "@types/node": "^20.8.10",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.9.1",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "prettier-eslint": "^16.1.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
